                type: "GET",
                async: false,

                success: function (JsonStr) {

                    var Json = eval("(" + JsonStr + ")");

                    for (var i = 0; i < Json.length; i++) {


                        //StriinnerHtml += "<div id='HoverSuccess'  style='width:220px; height:280px;margin:0;padding:19px;margin:15px 5px 0 2px;box-shadow:0px 2px 2px 2px lightgray;'> <div onclick='GoContent(" + Json[i].SuccessID + ")' style='width: 100 %;height:150px; background:url(" + Json[i].CollegeImg + ") center no-repeat;'></div>    <div style='font-size:18px; font-weight:600; color: #3298FD; height: 40px; line-height:40px;'>" + Json[i].StudentName + "</div><div style='height: 30px; line-height:30px; '><span style='font-weight :600;'>就读学院：</span>" + Json[i].JiuDuXueyuan + "</div><div style='height: 30px; line - height:30px; '><span style='font-weight:600;'>院校成绩：</span>" + Json[i].Score + "</div><div style='height: 30px; line-height:30px; '><span style='font-weight:600;'>录取院校：</span>" + Json[i].CollegeName + "</div></div>"

