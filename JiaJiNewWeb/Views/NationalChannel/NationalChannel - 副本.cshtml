                }
            })
            var countStudentnum = parseInt((countinfo / 4) * 1.0);
            var Loud = parseInt(countStudentnum * 4);

            if (countStudentnum / 4 > 0) {

                for (var j = 0; j < countStudentnum; j++) {

                    // if (j < countStudentnum) {
                    innerHtml_ul += "<li>";

                    LoadStudent(id,inedx, 4)
