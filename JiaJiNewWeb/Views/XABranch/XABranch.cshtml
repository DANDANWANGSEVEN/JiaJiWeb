        //验证表单
        function reg() {
            var country = $("#country").val();
            var date = $("#date").val();
            var uname = $("#uname").val();
            var tel = $("#tel").val();
            if (country == "" || date == "" || uname == "" || tel == "") {
                $("#tishi").empty();
                $("#tishi").text("不能为空！");
                return false;
            }
            ////验证是否有特殊字符
            //var reg2 = new RegExp(/[@@#\$%\^&\*]+\g/);
            //if (!reg2.test(country)) {
            //    $("#tishi").empty();
            //    $("#tishi").text("国家名称内不能有特殊字符！");
            //    return false;
            //}
            //验证日期格式
            var reg3 = new RegExp(/^\d{4}[年]\d{1,2}[月]\d{1,2}[日]$/);
            if (!reg3.test(date)) {
                $("#tishi").empty();
                $("#tishi").text("日期格式不正确！如：XXXX年XX月XX日");
                return false;
            }
            //验证真实姓名
            var reg1 = new RegExp(/^[\u4e00-\u9fa5]{2,4}$/);
            if (!reg1.test(uname)) {
                $("#tishi").empty();
                $("#tishi").text("请输入您的真实姓名！");
                return false;
            }


            //验证手机号
            var reg4 = new RegExp(/^[1][3578][\d]{9}$/);
            if (!reg4.test(tel)) {
                $("#tishi").empty();
                $("#tishi").text("手机号输入有误！");
                return false;
            }
            return true;

        }
        //主页背景轮播
