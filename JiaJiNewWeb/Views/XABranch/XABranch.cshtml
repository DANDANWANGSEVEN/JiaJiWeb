        (function ($) {
            $.fn.extend({
                textOverflow: function (options) {
                    var defaults = {
                        target: 'p'
                    }
                    var options = $.extend(defaults, options);
                    return this.each(function () {
                        var $this = $(this);
                        var $tag = $this.find(options.target);
                        while ($tag.outerHeight() > $this.height()) {
                            $tag.text($tag.text().replace(/(\s)*([a-zA-Z0-9]+|\W)(\.\.\.)?$/, '...'));
                        };
                    })
                }
            });
        })(jQuery);

